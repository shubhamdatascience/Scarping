    "dict_attributes = {'full_address':[],'price':[],'beds':[],'baths':[],'Sq.Ft':[],'price_per_Sq.Ft':[],'property_type':[],\n",
    "      'County':[],'community':[],'built':[],'Property_details':[]}\n",
    "for item in homeurl_number:\n",
    "    url = 'https://www.redfin.com' + item\n",
    "    cur = pycurl.Curl()\n",
    "    cur.setopt(pycurl.URL, url)\n",
    "    cur.setopt(pycurl.USERAGENT, \"Mozilla/5.0 (Macintosh; Intel Mac OS X 10_12_4) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/57.0.2987.133 Safari/537.36\")\n",
    "    bye = BytesIO()\n",
    "    cur.setopt(pycurl.WRITEFUNCTION, bye.write)\n",
    "    cur.perform()\n",
    "    content = bye.getvalue()\n",
    "    soup = bs4.BeautifulSoup(content,\"html.parser\")\n",
    "    for item in soup.find_all('meta'):\n",
    "        if (item.get('name') == 'twitter:text:baths'):\n",
    "            dict_attributes['baths'].append(item.get('content'))\n",
    "        if (item.get('name') == 'twitter:text:price'):\n",
    "            dict_attributes['price'].append(item.get('content'))\n",
    "        if (item.get('name') == 'twitter:text:sqft'):\n",
    "            dict_attributes['Sq.Ft'].append(item.get('content'))\n",
    "        if (item.get('name') == 'twitter:text:street_address'):\n",
    "            dict_attributes['full_address'].append(item.get('content'))\n",
    "        if (item.get('name') == 'twitter:text:beds'):\n",
    "            dict_attributes['beds'].append(item.get('content'))\n",
    "    for item in soup.find_all('span',class_ = 'header'):\n",
    "        if item.string == 'County':\n",
    "            id_county = str(int(item.get('data-reactid'))+1)\n",
    "        if item.string == 'Built':\n",
    "            id_built = str(int(item.get('data-reactid'))+1)\n",
    "        if item.string == 'Community':\n",
    "            id_community = str(int(item.get('data-reactid'))+1)\n",
    "        if item.string == 'Property Type':\n",
    "            id_type = str(int(item.get('data-reactid'))+1)\n",
    "    for item in soup.find_all('span',class_ = 'content'):\n",
    "        if (item.get('data-reactid') == id_type):\n",
    "            dict_attributes['property_type'].append(item.string)\n",
    "        if (item.get('data-reactid') == id_county):\n",
    "            dict_attributes['County'].append(item.string)\n",
    "        if (item.get('data-reactid') == id_community):\n",
    "            dict_attributes['community'].append(item.string)\n",
    "        if (item.get('data-reactid') == id_built):\n",
    "            dict_attributes['built'].append(item.string)\n",
    "    for item in soup.find_all('span'):\n",
    "        for label in item.find_all('div'):\n",
    "            if (label.get('data-rf-test-id') == 'abp-priceperft'):\n",
    "                dict_attributes['price_per_Sq.Ft'].append(label.string)\n",
    "    string = ''\n",
    "    for item in soup.find_all('div', class_ = 'amenity-group'):\n",
    "        string = string + item.get_text() + ' '\n",
    "    dict_attributes['Property_details'].append(string)\n",
    "    time.sleep(4)\n",
    "    \n",
    "print(dict_attributes)    "
   ]
